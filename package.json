	"config": {
		"ionic_copy": "./config/copy.config.js",
		"commitizen": {
			"path": "cz-conventional-changelog"
		},
		"ionic_manual_treeshaking": false
	},
	"dependencies": {
		"@angular/animations": "5.0.0",
		"@angular/common": "5.0.0",
		"@angular/compiler": "5.0.0",
		"@angular/compiler-cli": "5.0.0",
		"@angular/core": "5.0.0",
		"@angular/forms": "5.0.0",
		"@angular/http": "5.0.0",
		"@angular/platform-browser": "5.0.0",
		"@angular/platform-browser-dynamic": "5.0.0",
		"@angular/platform-server": "5.0.0",
		"ionic-angular": "3.9.2",
		"ionicons": "3.0.0",
		"rxjs": "5.5.2",
		"sw-toolbox": "3.6.0",
		"zone.js": "0.8.18",
		"cz-conventional-changelog": "2.1.0",
		"ionic-configuration-service": "../ionic-configuration-service/ionic-configuration-service-0.0.0-development.tgz",
		"ionic-configuration-viewer": "../ionic-configuration-viewer/ionic-configuration-viewer-0.0.0-development.tgz",
		"ionic-logging-service": "../ionic-logging-service/ionic-logging-service-0.0.0-development.tgz"
	},
	"devDependencies": {
		"@ionic/app-scripts": "3.1.2",
		"@types/cordova": "0.0.34",
		"codelyzer": "4.0.1",
		"ionic": "3.18.0",
		"semantic-release": "8.2.0",
		"tslint": "5.8.0",
		"typescript": "2.4.2"
	}
