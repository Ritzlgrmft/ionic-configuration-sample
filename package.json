{
	"name": "ionic-configuration-sample",
	"version": "0.0.0-development",
	"description": "Sample for ionic-configuration-service and ionic-configuration-viewer",
	"author": {
		"name": "Markus Wagner",
		"email": "markus.wagner02@web.de"
	},
	"repository": "https://github.com/Ritzlgrmft/ionic-configuration-sample.git",
	"license": "MIT",
	"private": false,
	"scripts": {
		"clean": "ionic-app-scripts clean",
		"build": "ionic-app-scripts build",
		"ionic:build": "ionic-app-scripts build",
		"ionic:serve": "ionic-app-scripts serve",
		"lint": "tslint --project tsconfig.json --format verbose --type-check",
		"test": "npm run lint && npm run ionic:build && npm run ionic:build --prod",
		"semantic-release": "semantic-release pre && semantic-release post"
	},
	"config": {
		"ionic_copy": "./config/copy.config.js",
		"commitizen": {
			"path": "cz-conventional-changelog"
		},
		"ionic_manual_treeshaking": false
	},
	"dependencies": {
		"@angular/animations": "4.1.3",
		"@angular/common": "4.1.3",
		"@angular/compiler": "4.1.3",
		"@angular/compiler-cli": "4.1.3",
		"@angular/core": "4.1.3",
		"@angular/forms": "4.1.3",
		"@angular/http": "4.1.3",
		"@angular/platform-browser": "4.1.3",
		"@angular/platform-browser-dynamic": "4.1.3",
		"@angular/platform-server": "4.1.3",
		"ionic-angular": "3.6.0",
		"ionicons": "3.0.0",
		"rxjs": "5.4.0",
		"sw-toolbox": "3.6.0",
		"zone.js": "0.8.12",
		"cz-conventional-changelog": "2.0.0",
		"ionic-configuration-service": "5.0.0",
		"ionic-logging-service": "3.1.0",
		"ionic-configuration-viewer": "4.1.1"
	},
	"devDependencies": {
		"@ionic/app-scripts": "2.1.4",
		"@types/cordova": "0.0.34",
		"codelyzer": "3.1.2",
		"ionic": "3.9.0",
		"semantic-release": "6.3.6",
		"tslint": "5.6.0",
		"typescript": "2.3.3"
	}
